public class StudentCollection {
	private int id;
	private String name;

	public StudentCollection(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return id + " " + name;
	}

	public static void main(String[] args) {
		List<StudentCollection> students = List.of(new StudentCollection(1, "Ranga"), new StudentCollection(100, "Adam"), new StudentCollection(2, "Eve"));
		System.out.println(students);
	}

}

